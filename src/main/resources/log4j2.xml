<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%blue{%d{HH:mm:ss}} [%t] %highlight{%-5level} %magenta{%logger{36}}- %msg%n"/>
        </Console>
    </Appenders>

    <Loggers>
        <Logger name="com.mindarray" level="DEBUG"/>
        <Root level="INFO">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>